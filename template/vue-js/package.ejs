{
  "name": "vite-template",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    <% if (useEslint) { -%>
    <%- EslintScript %>
    <% } -%>
    <% if (useVitest) { -%>
    <%- VitestScript %>
    <% } -%>
    <% if (usePrettier) { -%>
    <%- PrettierScript %>
    <% } -%>
    <% if (useTypeScript) { -%>
    <%- TypeScriptScript %>
    <% } -%>
  },
  "dependencies": {
    <%- constantProDeps %>
    <% if (useRouter) { -%>
    <%- Router %>
    <% } -%>
    <% if (usePinia) { -%>
    <%- Pinia %>
    <% } -%>
  },
  "devDependencies": {
    <% if (useTypeScript) { -%>
      <%- TypeScript %>
    <% } -%>
    <% if (useEslint) { -%>
      <%- Eslint %>
    <% } -%>
    <% if (usePrettier) { -%>
      <%- Prettier %>
    <% } -%>
    <% if (useTailwind) { -%>
      <%- Tailwind %>
    <% } -%>
    <% if (usePrettier && useEslint) { -%>
      <%- EslintWithPrettierScript %>
    <% } -%>
    <% if (useJavaScript) { -%>
      <%- JavaScript %>
    <% } -%>
    <% if (useVitest) { -%>
      <%- Vitest %>
    <% } -%>
    <%- constantDevDeps %>
    "vite": "^5.0.11",
    "@vitejs/plugin-vue": "^5.0.3"
  }
}
