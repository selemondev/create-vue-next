import { createApp } from 'vue'
<% if ( useTailwind ) { -%>
    import './assets/css/tailwind.css'
<% } -%>
<% if (!useTailwind ) { -%>
    import './assets/css/base.css'
<% } -%>
import App from './App.vue'
<% if ( usePinia ) { -%>
    import { createPinia } from 'pinia';
    <% } -%>
        <% if (useRouter) { -%>
            import router from "./router";
            <% } -%>
                const app = createApp(App)
                <% if ( usePinia ) { -%>
                    app.use(createPinia())
                    <% } -%>

                        <% if (useRouter) { -%>
                           app.use(router)
                            <% } -%>
                                app.mount('#app')